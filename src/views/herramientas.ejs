<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Herramientas</title>
</head>
<body>
    <h1>HERRAMIENTAS</h1>
    <ul>
        <% herramientas.forEach(function(herramienta) { %>
            <li><%= herramienta.nombre %> - <%= herramienta.marca %></li>
        <% }); %>
    </ul>

</body>
</html>